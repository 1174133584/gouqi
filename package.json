{
  "name": "gouqi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "quickbuild": "tsc",
    "test": "ava --verbose",
    "test:watch": "ava --watch --verbose",
    "coverage": "nyc ava --verbose && nyc report --reporter=html",
    "coverage:check": "open coverage/index.html"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "big-integer": "^1.6.15",
    "cookie": "^0.3.1",
    "lodash": "^4.15.0",
    "react": "15.3.1",
    "react-native": "0.33.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "request-promise": "^4.1.1",
    "tough-cookie": "^2.3.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "bluebird": "^3.4.6",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "mockery": "^1.7.0",
    "nyc": "^8.1.0",
    "react-dom": "^15.3.1",
    "react-native-mock": "^0.2.6",
    "request": "^2.74.0",
    "tslint": "^3.15.0-dev.0",
    "tslint-react": "^1.0.0",
    "typescript": "^2.1.0-dev.20160909"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./tests/setup"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/**/*.js.map"
    ]
  }
}
