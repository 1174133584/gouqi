{
  "name": "gouqi",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --configuration Release",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "quickbuild": "tsc",
    "dev": "node_modules/.bin/tsc  -w",
    "test": "ava --verbose",
    "test:watch": "ava --watch --verbose",
    "coverage": "nyc ava --verbose && nyc report --reporter=html",
    "coverage:check": "open coverage/index.html",
    "remotedev": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "postinstall": "npm run link && npm run remotedev"
  },
  "dependencies": {
    "antd-mobile": "^1.0.0",
    "big-integer": "^1.6.15",
    "blueimp-md5": "^2.4.0",
    "buffer": "^5.0.0",
    "crypto-js": "^3.1.6",
    "lodash": "^4.17.4",
    "qs": "^6.2.1",
    "rc-dialog": "^6.5.6",
    "react": "^15.4.2",
    "react-native": "0.42.0",
    "react-native-audio-streamer": "^0.0.7",
    "react-native-blur": "^2.0.0",
    "react-native-music-control": "^0.4.0",
    "react-native-navbar": "^1.5.4",
    "react-native-root-siblings": "^1.1.2",
    "react-native-router-flux": "^3.37.0",
    "react-native-scrollable-tab-view": "^0.6.0",
    "react-native-vector-icons": "^4.0.0",
    "react-native-video": "^1.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-saga": "^0.11.1",
    "timeago.js": "^2.0.5",
    "tough-cookie-no-native": "yuche/tough-cookie-no-native",
    "typescript": "2.2.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.31",
    "@types/lodash": "^4.14.52",
    "@types/qs": "^6.2.30",
    "@types/react": "^0.14.37",
    "@types/react-native": "^0.29.35",
    "@types/react-redux": "^4.4.32",
    "@types/redux-actions": "^0.8.30",
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-remove-console": "^6.8.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react-native": "^1.9.0",
    "bluebird": "^3.4.6",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mockery": "^1.7.0",
    "mockjs": "^1.0.1-beta3",
    "nyc": "^8.1.0",
    "react-dom": "^15.3.1",
    "react-native-mock": "^0.2.6",
    "remote-redux-devtools": "^0.5.7",
    "remote-redux-devtools-on-debugger": "^0.7.0",
    "tslint": "^3.15.0-dev.0",
    "tslint-react": "^1.0.0"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./tests/setup"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/**/*.js.map"
    ]
  }
}
